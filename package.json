{
  "name": "graphql-apollo-server-express-typescript-seed",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server-Express Typescript seed",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "npx eslint ./src/**",
    "prepare": "npm run generate",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js"
  },
  "keywords": [
    "apollo-server-express",
    "graphql",
    "typescript",
    "seed"
  ],
  "author": "Tal Shperling",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.3",
    "@graphql-codegen/typescript-resolvers": "1.19.4",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.12",
    "@types/graphql": "14.5.0",
    "@types/lodash": "4.14.170",
    "@types/mongodb": "3.6.18",
    "@types/node": "14.17.4",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "concurrently": "6.2.0",
    "eslint": "7.29.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "jsdoc": "3.6.7",
    "nodemon": "2.0.7",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "apollo-datasource-mongodb": "^0.4.0",
    "apollo-server-express": "^2.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^7.0.5",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "mongodb": "^3.6.3",
    "winston": "^3.3.3",
    "winston3-logstash-transport": "^1.2.3"
  }
}
